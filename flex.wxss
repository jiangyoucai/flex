/* border */

.b-none {
    border: none;
}

.b-none::after {
  border: none;
}

.b-border {
    border: 2rpx solid #f2f2f2;
}

.b-top {
    border-top: 2rpx solid #f2f2f2;
}

.b-right {
    border-right: 2rpx solid #f2f2f2;
}

.b-bottom {
    border-bottom: 2rpx solid #f2f2f2;
}

.b-left {
    border-left: 2rpx solid #f2f2f2;
}

/* border-radius */

.b-radius {
    border-radius: 12rpx;
}

.b-radiu1 {
    border-radius: 20rpx;
}

.b-radiu2 {
    border-radius: 40rpx;
}

.b-radiu3 {
    border-radius: 60rpx;
}

.b-radiu4 {
    border-radius: 80rpx
}

.b-radiu5 {
    border-radius: 100rpx
}

.b-radiux {
    border-radius: 50%;
}

/* border-shadow */

.b-shadow {
    box-shadow: 0 0 2rpx 2rpx #f2f2f2;
}

/* title */

.b-title {
    line-height: 1;
    border-left: 8rpx solid #4285f4;
    padding: 0 12rpx;
}

.c-title {
    position: relative;
}

.c-title span:before,
.c-title span:after {
    content: '';
    position: absolute;
    background: #c2c2c2;
    top: 50%;
    width: 15%;
    height: 2rpx;
}

.c-title span:before {
    left: 15%;
}

.c-title span:after {
    right: 15%;
}

/* image */

.i-center {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

/* overflow */

.o-auto {
    overflow: auto;
}

.o-hidden {
    overflow: hidden;
}

.o-none {
    outline: 0;
    -webkit-appearance: none;
}

/* position */

.p-absolute {
    position: absolute;
    z-index: 1;
}

.p-fixed {
    position: fixed;
    z-index: 1;
}

.p-relative {
    position: relative;
}

