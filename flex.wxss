/* text */

.t-current {
    color: #4285f4;
}

.t-hover:hover {
    color: #4285f4;
}

.t-pointer {
    cursor: pointer;
}

.t-ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

